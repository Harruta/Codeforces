#include <bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin >> t;
    for (int i = 0; i < t; i++)
    {
        string s;
        cin >> s;
        int length = s.size();
        if (s[length - 1] == 's' && s[length - 2] == 'u')
        {
            s[length - 2] = 'i';
            s = s.substr(0, length - 1);  // Truncate the last character 's'
        }
        cout << s << endl;
    }

    return 0;
}